<div class="tab-pane fade" id="settings-AccessSettings" role="tabpanel" aria-labelledby="access-settings-tab">
    <ul class="list-unstyled ps-0">
        <li class="mb-1">
            <button class="btn btn-toggle align-items-center rounded" data-bs-toggle="collapse"
                    data-bs-target="#users-collapse" aria-expanded="true">
                Users
            </button>


            <div class="collapse show" id="users-collapse" style="">
                @await Html.PartialAsync("Templates/CheckBox", new CheckBox("IsPublic", ViewBag.Workspace.IsPublic))

                @* @await Html.PartialAsync("Templates/InputField", new InputField("Test", ViewBag.Workspace.Test))
                @await Html.PartialAsync("Templates/CheckBox", new CheckBox("UseTest", ViewBag.Workspace.UseTest))
                @await Html.PartialAsync("Templates/Selector", new Selector("UseTest2", new List<string>(){"one", "two"})) *@

                <ul class="list-group mt-2" id="userlist">
                    <!-- Section generated by code -->
                </ul>

                <p class="mt-1 mb-1">Add user</p>
                <div class="d-flex flex-row">
                    <div class="col-md">
                        <div class="form-floating">
                            <input class="form-control" id="adduser-input">
                            <label for="adduser-input">Enter user name</label>
                        </div>
                    </div>
                    <button class="btn btn-outline-secondary ms-1" id="adduser-btn" onclick="AddUser()"><i class="bi bi-plus fs-3"></i></button>
                </div>
                <ul class="list-group mt-2" id="find-user-list">
                    <!-- Section generated by code -->
                </ul>
            </div>
        </li>
    </ul>
</div>