<div class="p-3 overflow-auto bg-white border-end item-panel tab-content">
	@if (!ViewBag.IsGuest)
    {
		@await Html.PartialAsync("Panels/DiagramsListPanel")
	}

	@await Html.PartialAsync("Panels/WorkspaceSettingsPanel")
	
	@if (ViewBag.IsAdmin)
    {
		@await Html.PartialAsync("Panels/AccessSettingsListPanel")
	}
	@await Html.PartialAsync("Panels/UserSettingsPanel")

</div>